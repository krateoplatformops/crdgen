{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
      "greeting": {
        "type": "string"
      },
      "display": {
        "type": "integer",
        "default": 1
      },
      "verbose": {
        "type": "boolean",
        "default": false
      },
      "url": {
        "type": "string",
        "default": "https://github.com/krateoplatformops/sticz"
      }
    },
    "required": [
      "greeting"
    ]
  }